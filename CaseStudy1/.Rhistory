set.seed(100)
cntTrainRecs = createDataPartition(y=df$y,p=0.8,list=FALSE)
train_df = df[cntTrainRecs,]
test_df = df[-cntTrainRecs,]
print(paste("Training Set Size: " ,dim(train_df)[1]))
