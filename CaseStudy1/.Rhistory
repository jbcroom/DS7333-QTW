df <- df %>% mutate(x32 = as.numeric(gsub("%", "", x32)))
df <- df %>% mutate(x37 = as.numeric(gsub("\\$", "", x37)))
df %>%
mutate(across(where(is.character), str_trim))
df = na.omit(df)
